{
  "ConnectionStrings": {
    "DbConnection": "server=localhost;userid=USERID;pwd=PASSWORD;port=3306;database=tlenbot;sslmode=none;"
  },

  "TelegramToken": "ENTER_YOUR_TELEGRAM_TOKEN_HERE",

  "Serilog": {
